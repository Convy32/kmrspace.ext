{
    "manifest_version": 2,
    "name": "WHS Kamar Theme",
    "version": "1.0",
    "description": "Applies a custom CSS theme to Kamar WHS",
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://kamar.whs.ac.nz/*"],
        "css": ["styles.css"]
      }
    ]
  }